# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_MODEL=gpt-4-turbo-preview

# Email Service (Resend)
RESEND_API_KEY=re_your-resend-api-key
RESEND_FROM_EMAIL=team@fitcircle.app
RESEND_REPLY_TO=support@fitcircle.app

# Push Notifications
ONESIGNAL_APP_ID=your-onesignal-app-id
ONESIGNAL_REST_API_KEY=your-onesignal-api-key

# Redis (Upstash)
REDIS_URL=redis://default:password@your-redis-endpoint.upstash.io:6379
REDIS_TOKEN=your-upstash-token

# Sentry
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# App Configuration
NEXT_PUBLIC_APP_URL=https://fitcircle.app
NEXT_PUBLIC_API_URL=https://fitcircle.app/api
NODE_ENV=production

# JWT Configuration
JWT_SECRET=your-jwt-secret-min-32-chars-long
JWT_REFRESH_SECRET=your-refresh-secret-min-32-chars-long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# AI Features
AI_PROVIDER=openai
ANTHROPIC_API_KEY=sk-ant-your-claude-api-key

# Storage
STORAGE_BUCKET_NAME=fitcircle-media
CDN_URL=https://cdn.fitcircle.app

# Feature Flags
ENABLE_AI_COACHING=true
ENABLE_PREMIUM_FEATURES=true
ENABLE_BETA_FEATURES=false